<concept id="gfname" xml:lang="en-US">
   <title id="title__Toc50995181">GFNAME System Variable</title>
   <conbody>
      <section><title>Gradient fill name</title><p>Specifies the pattern of a gradient fill.</p><p><table id="table_r32_ncz_hnb">
               <tgroup cols="2">
                  <colspec colname="c1" colnum="1" colwidth="1*" />
                  <colspec colname="c2" colnum="2" colwidth="3*" />
                  <tbody>
                     <row>
                        <entry conkeyref="system_variable_warehouse/type" />
                        <entry><p conkeyref="system_variable_warehouse/short" /></entry>
                     </row>
                     <row>
                        <entry conkeyref="system_variable_warehouse/saved_in" />
                        <entry><p conkeyref="system_variable_warehouse/not_saved" /></entry>
                     </row>
                     <row>
                        <entry conkeyref="system_variable_warehouse/range" />
                        <entry>
                           <p>1 to 9</p>
                        </entry>
                     </row>
                     <row>
                        <entry conkeyref="system_variable_warehouse/default_value" />
                        <entry><p>1</p></entry>
                     </row>
                     <row>
                        <entry conkeyref="system_variable_warehouse/possible_values" />
                        <entry>
                           <p>1: LINEAR</p>
                           <p>2: CYLINDER</p>
                           <p>3: INVCYLINDER</p>
                           <p>4: SPHERICAL</p>
                           <p>5: HEMISPHERICAL</p>
                           <p>6: CURVED</p>
                           <p>7: INVSPHERICAL</p>
                           <p>8: INVHEMISPHERICAL</p>
                           <p>9: INVCURVED</p>
                        </entry>
                     </row>
                  </tbody>
               </tgroup>
            </table></p></section>
   </conbody>
</concept>
